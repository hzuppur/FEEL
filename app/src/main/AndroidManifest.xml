<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.feel">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".FeelPositivityActivity"
            android:label="Feeling positivity">
        </activity>
        <activity
            android:name=".FeelIntensityActivity"
            android:label="Feeling intensity">
        </activity>
        <activity
            android:name=".FeelBodyActivity"
            android:label="Where in body">
        </activity>
        <activity
            android:name=".FeelTriggerActivity"
            android:label="What triggered you">
        </activity>
        <activity
            android:name=".FeelEmotionActivity"
            android:label="Emotion">
        </activity>
        <activity
            android:name=".FeelWhenActivity"
            android:label="When">
        </activity>
        <activity
            android:name=".FeelReactedActivity"
            android:label="How you reacted">
        </activity>
        <activity
            android:name=".FeelUnderstandOrBetter"
            android:label="Understand feeling or feel better">
        </activity>
    </application>

</manifest>